.container {
  margin: 20rpx 20rpx 0 20rpx;
  display: flex;
  flex-direction: row;
}

.container-left image {
  width: 300rpx;
  height: 300rpx;
}

.container-right {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.container-right-t text {
  color: #000;
  font-weight: bolder;
}

.container-right-b text {
  color: #666;
}
